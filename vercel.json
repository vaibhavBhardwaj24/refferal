{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/assets/.*\\.js",
      "headers": { "content-type": "application/javascript" }
    },
    {
      "src": "/assets/.*\\.css",
      "headers": { "content-type": "text/css" }
    }
  ]
}
